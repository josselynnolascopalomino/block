   "source": [
    "import numpy as np\n",
    "\n",
    "A = [5.2,5.8,5.9]\n",
    "Ar = np.array(A)\n",
    "print(Ar)\n",
    "sum(Ar)\n",
    "media = np.mean(Ar)\n",
    "print(media)\n",
    "varianza = np.var(Ar)\n",
    "print(varianza)\n",
    "B = np.array([[1, 2, 3], [4, 5, 6]])\n",
    "print(B)\n",
    "print(np.mean(B[0]))\n",
    "print(np.mean(B,axis=1))\n",
    "print(np.mean(B,axis=0))\n",
    "print(B.mean(axis=1))#columna\n",
    "print(B.mean(axis=0))#fila\n",
    "print(B[:,0].mean())                        "
