   "outputs": [],
   "source": [
    "A = [5.2,5.8,5.9]\n",
    "## Recordemos funciones para trabajar listas\n",
    "print(type(A))#tipo de A\n",
    "print(len(A))#lonfgitud de A\n",
    "print(A)\n",
    "A.append(10.9)#agrega un elemento \n",
    "print(A)\n",
    "print(len(A))\n",
    "A = [5.2,5.8,5.9,15.6,8.975]\n",
    "A.count(5.2)\n",
